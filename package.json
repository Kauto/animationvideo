{
  "name": "animationvideo",
  "amdName": "AnimationVideo",
  "version": "0.10.1",
  "description": "Javascript-Libary for animation and audio syncing",
  "main": "dist/animationvideo.js",
  "browser": "dist/animationvideo.umd.js",
  "umd:main": "dist/animationvideo.umd.js",
  "module": "dist/animationvideo.m.js",
  "source": "src/animationvideo.js",
  "scripts": {
    "build": "microbundle && microbundle -f umd --external none",
    "dev": "microbundle watch --external none",
    "test": "NODE_ENV=test ./node_modules/mocha/bin/_mocha --compilers js:babel-core/register --ui bdd --recursive ./test"
  },
  "keywords": [
    "animation",
    "canvas",
    "audio",
    "tween"
  ],
  "sideEffects": false,
  "author": "Markus Madeja",
  "repository": "Kauto/animationvideo",
  "license": "MIT",
  "dependencies": {
    "color": "^3.1.0",
    "eases": "^1.0.8",
    "pasition": "^1.0.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "microbundle": "^0.7.0",
    "mocha": "^5.2.0"
  }
}
