  },
  "keywords": [
    "animation",
    "canvas",
    "audio",
    "tween"
  ],
  "sideEffects": false,
  "author": "Markus Madeja",
  "repository": "Kauto/animationvideo",
  "homepage": "http://intergenies.com/programs/nodejs/animationvideo",
  "license": "MIT",
  "dependencies": {
    "color": "^3.1.2",
    "eases": "^1.0.8",
    "pasition": "^1.0.1",
    "stackblur-canvas": "^2.2.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "microbundle": "^0.11.0",
    "mocha": "^6.2.0"
  }
}
